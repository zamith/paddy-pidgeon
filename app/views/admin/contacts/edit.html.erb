<h1><%= t('admin.contacts.edit.title') %></h1>

<%= form_for @contact, url: admin_contact_path(@contact), method: :put do |form| %>
  <%= render partial: 'form', locals: { form: form } %>

  <%= form.submit t('admin.actions.edit') %>
  <%= link_to t('admin.actions.back'), admin_contact_path(@contact) %>
<% end %>
